<?xml version="1.0" encoding="UTF-8" ?>

<web-app>
    <servlet>
        <servlet-name>CryptoPage</servlet-name>
        <servlet-class>de.hswt.swa.cryptotool.website.CryptoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CryptoPage</servlet-name>
        <url-pattern>/crypto</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CryptoService</servlet-name>
        <servlet-class>de.hswt.swa.cryptotool.api.CryptoApiService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CryptoService</servlet-name>
        <url-pattern>/crypto-service</url-pattern>
    </servlet-mapping>
</web-app>